<h3>Question Response Rates:</h3>
<ul>
<% survey.questions.each do |question| %>
  <li>
    <span><%=question.text%></span>
    <p>
      <span class='<%=question.adequate_responses? ? "adequate_responses" : "inadequate_responses" %>'>
        <%=question.response_count%> of <%=question.minimum_responses%>
      </span>required responses
    </p>
  </li>
<% end %>
</ul>
