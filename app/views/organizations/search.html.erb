<h2>Members</h2>
<h3>Search Members</h3>
<%= render :partial => "search_form" %>
<h3>Search Results</h3>
<% if @organizations.size == 0 then %>
  <p>Your search returned zero results</p>
<% else %>
  <%= render :partial => "organization_list", :locals => { :organizations => @organizations } %>
<% end %>
